{
    "name": "multidots/laravel-admin",
    "description": "laravel admin",
    "authors": [
        {
            "name": "vikash",
            "email": "vikram.solanki@multidots.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.0.0",
        "laravelcollective/html": "5.6.*"
    },
    "autoload": {
        "psr-4": {
